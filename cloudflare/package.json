{
  "name": "dunbar-tss-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "db:push": "drizzle-kit push --config src/server/db/config.ts",
    "db:studio": "drizzle-kit studio --config src/server/db/config.ts",
    "db:generate": "drizzle-kit generate --config src/server/db/config.ts",
    "db:migrate": "drizzle-kit migrate --config src/server/db/config.ts",
    "cf:migrate": "wrangler d1 migrations apply DB --local",
    "cf:deploy": "wrangler deploy",
    "cf:types": "wrangler types"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-ssr-query": "^1.132.47",
    "@tanstack/react-start": "^1.132.53",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.12",
    "@libsql/client": "^0.15.15",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-devtools": "^0.7.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.132.51",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.42.2"
  }
}
