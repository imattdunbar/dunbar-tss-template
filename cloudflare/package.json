{
  "name": "dunbar-tss-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "db:push": "drizzle-kit push --config src/db/config.ts",
    "db:studio": "drizzle-kit studio --config src/db/config.ts",
    "db:generate": "bun run src/core/scripts/db-generate.ts",
    "db:migrate": "drizzle-kit migrate --config src/db/config.ts",
    "db:clone-remote": "bun run src/core/scripts/db-clone-remote.ts",
    "cf:deploy": "wrangler deploy",
    "cf:types": "wrangler types  --env-interface=CloudflareEnv"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-router": "^1.134.12",
    "@tanstack/react-router-ssr-query": "^1.134.12",
    "@tanstack/react-start": "^1.134.12",
    "better-auth": "^1.3.34",
    "dayjs": "^1.11.19",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.10.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.19",
    "@libsql/client": "^0.15.15",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-devtools": "^0.8.0",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.134.12",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.45.4"
  }
}
